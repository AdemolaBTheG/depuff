{
  "expo": {
    "name": "depuff",
    "slug": "depuff",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",

    "scheme": "depuff",
    "userInterfaceStyle": "automatic",
    "ios": {
      "icon": "./assets/images/depuff.icon",

      "supportsTablet": false,
      "bundleIdentifier": "com.depuff.app",
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to the camera to allow you to take photos and videos and analyze them.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "$(PRODUCT_NAME) may not actually need this permission, but it may be required by one of our services."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "predictiveBackGestureEnabled": false,
      "permissions": ["android.permission.CAMERA"]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-image",
      "expo-secure-store",
      "expo-sharing",
      "expo-sqlite",
      "expo-web-browser",
      "expo-localization",

      "react-native-vision-camera",

      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "949b5347-9bde-4d72-99a4-a2f8e3ccbe99"
      }
    }
  }
}
